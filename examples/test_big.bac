func main() {
    let x = 10;
    let y = 5;
    let z = x + y;

    print(z);  // 15

    if (z > 10) {
        print("z is greater than 10");
    } else {
        print("z is 10 or less");
    }

    let count = 0;
    while (count < 3) {
        print(count);
        count = count + 1;
    }

    for (let i = 0; i < 3; i = i + 1) {
        print(i);
    }

    return z;
}
